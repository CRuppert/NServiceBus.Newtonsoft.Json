<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
    <NoWarn>0419</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\GitVersion.yml" Link="GitVersion.yml"/>
    <None Include="..\..\packaging\nuget\NServiceBus.Newtonsoft.Json.nuspec" Link="NServiceBus.Newtonsoft.Json.nuspec" />
    <PackageReference Include="GitVersionTask" Version="4.0.0-beta0011" />
    <PackageReference Include="NuGetPackager" Version="0.6.1" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.2" />
    <PackageReference Include="NServiceBus" Version="6.3.1" />
  </ItemGroup>
</Project>